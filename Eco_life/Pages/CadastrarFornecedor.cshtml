@page
@model Eco_life.Pages.CadastrarFornecedorModel
@{
    ViewData["Title"] = "Cadastro de Fornecedor";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Cadastro de Fornecedor</title>
    <link rel="stylesheet" href="/css/site.css" />
</head>
<body>
    <h1>Cadastro de Fornecedor</h1>
    <form method="post">
        <label for="nome">Nome:</label>
        <input type="text" name="Nome_Funcionario" required /><br />

        <label for="email">Email:</label>
        <input type="email" name="Email_Funcionario" required /><br />

        <label for="senha">Senha:</label>
        <input type="password" name="Senha_Funcionario" required /><br />

        <label for="token">Token:</label>
        <input type="text" name="Token" /><br />

        <button type="submit">Criar conta</button>
    </form>
</body>
</html>
